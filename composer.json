{
    "name": "jahvi/magento2-copy-theme-override",
    "description": "Magento 2 command to automatically copy files into active theme",
    "authors": [
        {
            "name": "Javier Villanueva",
            "email": "javiervd@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "php": "^7.3",
        "magento/framework": "^102|^103",
        "symfony/console": "^4.1"
    },
    "require-dev": {
        "php-mock/php-mock-phpunit": "^2",
        "phpunit/phpunit": "^8",
        "satooshi/php-coveralls": "^1.0"
    },
    "type": "magento2-module",
    "version": "2.0.1",
    "license": [
        "MIT"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Jahvi\\CopyThemeOverride\\": ""
        }
    }
}
